{
  "name": "eventos-backend-mysql",
  "version": "1.0.0",
  "description": "Backend para sistema de gesti√≥n de eventos con MySQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "mysql -u root -p < init.sql",
    "init-db_long": "mysql -u root -p < init_extenso.sql",
    "prueba.get": "npx autocannon http://localhost:5000/api/asistentes -c 100 -d 20",
    "prueba.post": "npx autocannon http://localhost:5000/api/eventos -m POST -H 'Content-Type: application/json' -i 'evento-body.json'",
    "prueba.put": "npx autocannon http://localhost:5000/api/eventos -c 50 -d 20"
  },
  "dependencies": {
    "autocannon": "^8.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "keywords": [
    "mysql",
    "eventos",
    "crud"
  ],
  "author": "",
  "license": "ISC"
}
